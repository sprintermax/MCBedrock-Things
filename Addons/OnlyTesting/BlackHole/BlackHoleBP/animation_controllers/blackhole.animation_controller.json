            "/execute @e[type=!Player] ~ ~ ~ tp @s[type=!sprintermax:blackhole] ^0.5 ^ ^0.1 facing @e[type=sprintermax:blackhole,c=1]",
            "/execute @a[m=!1] ~ ~ ~ tp @s[type=!sprintermax:blackhole] ^0.5 ^ ^0.1 facing @e[type=sprintermax:blackhole,c=1]",
            "/kill @e[type=!sprintermax:blackhole,r=1.75]"

  "hud_content": {
    "bindings": [
      {
        "binding_name": "#hud_title_text_string"
      },
      {
        "binding_name": "#hud_subtitle_text_string"
      },
      {
        "binding_type": "view",
        "source_property_name": "(((#hud_title_text_string - $hudplus_global_noui_prefix) = #hud_title_text_string) and ((#hud_subtitle_text_string - $hudplus_global_noui_prefix) = #hud_subtitle_text_string))",
        "target_property_name": "#visible"
      }
    ]
  }


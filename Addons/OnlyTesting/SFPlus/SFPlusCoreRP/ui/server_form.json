{
  "third_party_server_screen@common.base_screen": {
    "$screen_content": "common.base_screen_empty_panel",
    "$screen_bg_content": "sfplus_screen_defs.custom_screens",
    "$background_animations": [],
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      }
    ]
  },

  "long_form": {
    "$use_custom_title_control": true,
    "$custom_title_label": "sfplus_common.standard_title_label_no_prefix"
  },

  "custom_form": {
    "$use_custom_title_control": true,
    "$custom_title_label": "sfplus_common.standard_title_label_no_prefix"
  }
}
